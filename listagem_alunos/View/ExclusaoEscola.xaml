<Window x:Class="listagem_alunos.View.ExclusaoEscola"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:listagem_alunos.View"
        mc:Ignorable="d"
        Title="ExclusaoEscola" Height="135" Width="549">
    <Grid Margin="0,0,0,-6">
        <Label Content="Tem certeza que deseja excluir essa escola? Todos seus alunos serão excluídos automaticamente" HorizontalAlignment="Left" Height="44" Margin="10,10,0,0" VerticalAlignment="Top" Width="530"/>
        <Button Content="Sim" HorizontalAlignment="Left" Margin="145,54,0,0" VerticalAlignment="Top" Click="Button_Click" Height="23" Width="83"/>
        <Button Content="Não" HorizontalAlignment="Left" Height="23" Width="83" Margin="322,54,0,0" VerticalAlignment="Top"  Click="Button_Click_1"/>

    </Grid>
</Window>
